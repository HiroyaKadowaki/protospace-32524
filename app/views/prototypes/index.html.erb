<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        <%= "こんにちわ" %>
        <%= link_to current_user.name+"さん", user_path(current_user), class: :greeting_link%>
      </div>
    <% end %>
    <div class="card__wrapper">
      <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
      <% @prototypes.each do |prototype| %>
        <%= render partial: "prototypes/prototype", locals: { prototype:prototype } %>
      <% end %>
    </div>
  </div>
</main>
